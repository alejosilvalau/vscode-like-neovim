{
  "editor.fontFamily": "CaskaydiaMono Nerd Font",
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.minimap.enabled": false,
  "git.ignoreMissingGitWarning": true,
  "editor.fontSize": 15,
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabCompletion": "on",
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "window.titleBarStyle": "custom",
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.stickyScroll.enabled": false,
  "sqltools.format": {},
  "sqltools.results.reuseTabs": "connection",
  "workbench.panel.showLabels": false,
  "sqltools.autoOpenSessionFiles": false,
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "cSpell.customDictionaries": {},
  "cSpell.language": "en,es-ES",
  "editor.lineNumbers": "relative",
  "sqltools.connections": [],
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.replaceWithRegister": true,
  "vim.showMarksInGutter": true,
  "vim.visualstar": true,
  "vim.handleKeys": { "<C-c>": false, "<C-v>": false, "<C-a>": false, "<C-f>": false, "<C-w>": false },
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "sqltools.queryParams.enableReplace": true,
  "sqltools.tableTreeItemsExpanded": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.notebook.enhancedNextEditSuggestions.enabled": true,
  "editor.hover.sticky": false,
  "editor.stickyScroll.enabled": false,
  "python.createEnvironment.trigger": "off",
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "zenMode.centerLayout": false,
  "window.customTitleBarVisibility": "windowed",
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "Tokyo Night",
  "zenMode.showTabs": "none",
  "zenMode.hideStatusBar": false,
  "editor.formatOnSave": true,
  "settingsSync.keybindingsPerPlatform": false
}